{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "username",
        "password",
        "firstName",
        "lastName",
        "email"
    ],
    "properties": {
        "username": {
            "type": "string",
            "default": "",
            "minLength": 1,
            "maxLength": 30,
            "title": "The username Schema",
            "examples": [
                "Taiohk31"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "minLength": 1,
            "maxLength": 20,
            "examples": [
                "Luckytaioh1234!"
            ]
        },
        "firstName": {
            "type": "string",
            "default": "",
            "title": "The firstName Schema",
            "minLength": 1,
            "maxLength": 30,
            "examples": [
                "Taioh"
            ]
        },
        "lastName": {
            "type": "string",
            "default": "",
            "title": "The lastName Schema",
            "minLength": 1,
            "maxLength": 30,
            "examples": [
                "Kubota"
            ]
        },
        "email": {
            "type": "string",
            "default": "",
            "title": "The email Schema",
            "minLength": 1,
            "maxLength": 60,
            "format": "email",
            "examples": [
                "kubotataioh@gmail.com"
            ]
        }
    },
    "examples": [{
        "username": "Taiohk31",
        "password": "Luckytaioh1234!",
        "firstName": "Taioh",
        "lastName": "Kubota",
        "email": "kubotataioh@gmail.com"
    }]
}
